<%= form_for @song do |f| %>
     <%= f.label :artist, "Artist"  %>
     <%= f.text_field :artist, class: 'block'  %>

     <%= f.label :album, "Album"  %>
     <%= f.text_field :album, class: 'block'  %>

     <%= f.label :title, "Song Name"  %>
     <%= f.text_field :title, class: 'block'  %>

     <%= f.label :photo_url, "Photo Url"  %>
     <%= f.text_field :photo_url, class: 'block'  %>

     <%= f.submit %>
<% end %>

<%= link_to 'Back', songs_path %>